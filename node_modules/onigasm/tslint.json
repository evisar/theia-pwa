{
    "extends": "tslint:recommended",
    "jsRules": { },
    "rules": {
        "semicolon": false,
        "max-line-length": false,
        "arrow-parens": false,
        "quotemark": false,
        "variable-name": false,
        "no-require-imports": false,
        "no-bitwise": false,
        "no-empty": false,
        "prefer-for-of": false,
        "no-var-requires": false,
        "one-line": false
    }
}